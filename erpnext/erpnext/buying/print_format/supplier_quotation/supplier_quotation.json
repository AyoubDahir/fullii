 "html": "<style>\r\n.logo{\r\n    \r\n    margin-top: 0px;\r\n}\r\n    .header-title {\r\n        text-align: center;\r\n        font-size: 24px;\r\n        font-weight: bold;\r\n        margin-bottom: 5px;\r\n    }\r\n\r\n    .amended-from {\r\n        text-align: center;\r\n        font-style: italic;\r\n        font-size: 14px;\r\n        margin-bottom: 20px;\r\n    }\r\n\r\n    .info-table {\r\n        width: 100%;\r\n        margin-bottom: 15px;\r\n        font-size: 14px;\r\n    }\r\n\r\n    .info-table td {\r\n        padding: 4px 8px;\r\n        vertical-align: top;\r\n    }\r\n\r\n    .section-title {\r\n        text-align: center;\r\n        font-weight: bold;\r\n        font-size: 16px;\r\n        margin-top: 20px;\r\n        margin-bottom: 5px;\r\n    }\r\n\r\n    .warehouse {\r\n        text-align: center;\r\n        font-size: 14px;\r\n        margin-bottom: 20px;\r\n    }\r\n</style>\r\n\r\n\r\n<div class = \"logo\">\r\n{{ letter_head }}\r\n</div>\r\n<div class=\"header-title\">Supplier Quotation</div>\r\n<div class=\"amended-from\">\r\n    {% if doc.amended_from %}\r\n        {{ doc.amended_from }}\r\n    {% else %}\r\n         None\r\n    {% endif %}\r\n</div>\r\n\r\n<table class=\"info-table\">\r\n    <tr>\r\n        <td><strong>User:</strong> {{ doc.owner }}</td>\r\n        <td><strong>Transaction Date:</strong> {{ doc.transaction_date }}</td>\r\n    </tr>\r\n    <tr>\r\n        <td><strong>Full Name:</strong> {{ frappe.db.get_value(\"User\", doc.owner, \"full_name\") }}</td>\r\n        <td><strong>Valid till:</strong> {{ doc.valid_till }}</td>\r\n    </tr>\r\n    <!--<tr>-->\r\n    <!--    <td colspan=\"2\"><strong>Purpose:</strong> {{ doc.material_request_type }}</td>-->\r\n    <!--</tr>-->\r\n      <tr>\r\n        <td colspan=\"2\"><strong>Supplier name:</strong> {{ doc.supplier_name }}</td>\r\n    </tr>\r\n</table>\r\n\r\n<!--<div class=\"section-title\">Set Source Warehouse</div>-->\r\n<!--<div class=\"warehouse\">-->\r\n<!--    {{ doc.set_from_warehouse or \"N/A\" }}-->\r\n<!--</div>-->\r\n\r\n\r\n<h3>Items</h3>\r\n\r\n<table style=\"width: 100%; border-collapse: collapse;\" border=\"1\" cellpadding=\"5\">\r\n    <thead>\r\n        <tr> \r\n             <th>No</th>\r\n            <th>Item Code</th>\r\n            <th>Qty</th>\r\n            <th>Warehouse</th>\r\n            <th>UOM</th>\r\n            <th>Conversion Factor</th>\r\n            <th>Stock UOM</th>\r\n        </tr>\r\n    </thead>\r\n    <tbody>\r\n        {% for item in doc.items %}\r\n        <tr>\r\n            <td>{{ loop.index }}</td>\r\n            <td>{{ item.item_code }}</td>\r\n            <td>{{ item.qty }}</td>\r\n            <td>{{ item.warehouse }}</td>\r\n            <td>{{ item.uom }}</td>\r\n            <td>{{ item.conversion_factor }}</td>\r\n            <td>{{ item.stock_uom }}</td>\r\n        </tr>\r\n        {% endfor %}\r\n    </tbody>\r\n</table>\r\n",
